{
  "name": "o2diff",
  "version": "5.0.0",
  "description": "Returns the difference between two objects",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "prepublish": "pnpm build",
    "build": "pnpm rollup -c",
    "prettify": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "pnpm lint && pnpm prettify",
    "lint": "eslint",
    "test": "NODE_ENV=test jest",
    "coverage": "nyc pnpm test",
    "coverage:codecov": "nyc --reporter=lcov pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderMac/o2diff.git"
  },
  "keywords": [
    "object",
    "diff",
    "difference",
    "compare",
    "changes"
  ],
  "author": "Alexander Mac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderMac/o2diff/issues"
  },
  "homepage": "https://github.com/AlexanderMac/o2diff",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.10",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "rollup": "^4.32.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  }
}
